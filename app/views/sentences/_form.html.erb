<%= simple_form_for(@sentence) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :story_id %>
    <%= f.input :author_id %>
    <%= f.input :content %>
    <%= f.input :length %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
